{
 "$schema": "vscode://schemas/settings/user",

 // General
 //
 // Editor
 "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
 "editor.unicodeHighlight.ambiguousCharacters": false,
 "editor.codeActionsOnSave": {
  "source.fixAll": "never",
  "source.organizeImports": "never"
 },
 "editor.cursorBlinking": "expand",
 "editor.cursorSmoothCaretAnimation": "on",
 "editor.cursorStyle": "line",
 "editor.cursorWidth": 3,
 "editor.colorDecorators": true,
 "editor.formatOnSave": true,
 "editor.find.addExtraSpaceOnTop": false,
 "editor.guides.bracketPairs": true,
 "editor.gotoLocation.multipleDefinitions": "goto",
 "editor.gotoLocation.multipleImplementations": "goto",
 "editor.inlayHints.enabled": "off",
 "editor.inlineSuggest.enabled": true,
 "editor.linkedEditing": true,
 "editor.minimap.enabled": false,
 "editor.multiCursorModifier": "ctrlCmd",
 "editor.stickyScroll.enabled": true,
 "editor.suggestSelection": "first",
 "editor.tabCompletion": "on",
 "editor.snippetSuggestions": "top",
 "editor.renderWhitespace": "boundary",
 "editor.renderControlCharacters": false,

 // Window
 "window.newWindowProfile": "Predeterminado",

 // Explorer
 "explorer.compactFolders": false,

 // Security
 "security.allowedUNCHosts": ["wsl.localhost"],
 "security.workspace.trust.untrustedFiles": "open",

 // Formatting
 "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[json5]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[jsonl]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
 "[go]": { "editor.defaultFormatter": "golang.go" },

 // Langs
 //
 // Typescript Server
 "typescript.tsserver.experimental.enableProjectDiagnostics": false,
 "typescript.tsserver.log": "off",
 "typescript.tsserver.maxTsServerMemory": 2048,
 "typescript.tsserver.watchOptions": {
  "watchDirectory": "useFsEvents",
  "watchFile": "useFsEvents"
 },

 // Typescript and Javascript
 "javascript.inlayHints.functionLikeReturnTypes.enabled": false,
 "javascript.inlayHints.parameterNames.enabled": "none",
 "javascript.inlayHints.variableTypes.enabled": false,
 "javascript.preferences.useAliasesForRenames": false,
 "javascript.suggest.autoImports": true,
 "javascript.updateImportsOnFileMove.enabled": "never",

 "typescript.inlayHints.enumMemberValues.enabled": true,
 "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
 "typescript.inlayHints.parameterNames.enabled": "none",
 "typescript.inlayHints.variableTypes.enabled": false,
 "typescript.preferences.useAliasesForRenames": false,
 "typescript.suggest.autoImports": true,
 "typescript.updateImportsOnFileMove.enabled": "never",

 // Go
 "go.formatTool": "goimports",
 "go.inlayHints.functionTypeParameters": true,
 "go.inlayHints.parameterNames": true,
 "go.lintOnSave": "package",
 "go.lintTool": "golangci-lint",
 "go.testOnSave": false,
 "go.useLanguageServer": true,

 // Productivity
 //
 // Git
 "git.autofetch": true,
 "git.confirmSync": false,
 "git.enableSmartCommit": true,
 "git.ignoreMissingGitWarning": true,
 "git.pullBeforeCheckout": true,

 // Files
 "files.insertFinalNewline": true,
 "files.trimTrailingWhitespace": true,
 "files.exclude": {
  "**/.DS_Store": true,
  "**/Thumbs.db": true
 },
 "files.watcherExclude": {
  "**/.git/**": true,
  "**/.next/**": true,
  "**/.turbo/**": true,
  "**/build/**": true,
  "**/dist/**": true,
  "**/node_modules/**": true,
  "**/out/**": true
 },

 // Search
 "search.exclude": {
  "**/node_modules": true,
  "**/dist": true,
  "**/build": true
 },
 "search.quickOpen.includeSymbols": true,

 // Workbench
 "workbench.colorTheme": "01e61500-862c-41cd-b653-48d2ef7b1882",
 "workbench.editor.enablePreview": true,
 "workbench.editor.pinnedTabsOnSeparateRow": true,
 "workbench.iconTheme": "material-icon-theme",
 "workbench.sideBar.location": "right",
 "workbench.startupEditor": "none",

 // Terminal
 "terminal.integrated.cursorStyle": "line",
 "terminal.integrated.cursorWidth": 2,
 "terminal.integrated.scrollback": 2000,
 "terminal.integrated.stickyScroll.enabled": true,
 "terminal.integrated.tabs.focusMode": "singleClick",

 // Debug
 "debug.inlineValues": "on",
 "debug.openDebug": "neverOpen",

 // Extensions
 //
 // VScord
 "vscord.app.name": "Code",
 "vscord.status.buttons.button1.git.active.url": "https://github.com/r4lrgx",
 "vscord.status.buttons.button1.git.idle.url": "https://github.com/r4lrgx",
 "vscord.status.buttons.button1.git.inactive.url": "https://github.com/r4lrgx",
 "vscord.status.details.text.debugging": "üîß Debugging {file_name}{file_extension}",
 "vscord.status.details.text.editing": "üìÇ In .r4lrgx {problems}",
 "vscord.status.details.text.idle": "üí§ Idling",
 "vscord.status.details.text.noWorkSpaceText": "üìÇ no workspace",
 "vscord.status.details.text.notInFile": "ü™¶ Not in a file!",
 "vscord.status.details.text.viewing": "üìÇ In .r4lrgx {problems}",
 "vscord.status.idle.timeout": 500,
 "vscord.status.image.large.debugging.text": "üîß Debugging a {LANG} file",
 "vscord.status.image.large.editing.text": "üìÑ Editing a {LANG} file",
 "vscord.status.image.large.idle.text": "üí§ Idling",
 "vscord.status.image.large.viewing.text": "üëÄ Viewing a {LANG} file",
 "vscord.status.image.small.debugging.text": "‚å®Ô∏è {app_name}",
 "vscord.status.image.small.editing.key": "https://raw.githubusercontent.com/LeonardSSH/vscord/main/assets/icons/vscode-insiders.png",
 "vscord.status.image.small.editing.text": "‚å®Ô∏è {app_name}",
 "vscord.status.image.small.idle.text": "ü™¶ zZz",
 "vscord.status.image.small.notInFile.text": "üí§ Snoozin...",
 "vscord.status.image.small.viewing.key": "https://raw.githubusercontent.com/LeonardSSH/vscord/main/assets/icons/vscode-insiders.png",
 "vscord.status.image.small.viewing.text": "‚å®Ô∏è {app_name}",
 "vscord.status.problems.text": "- üï∑Ô∏è {problems_count} problems found",
 "vscord.status.state.text.debugging": "üîß Debugging: .r4lrgx",
 "vscord.status.state.text.editing": "üìÑ Working on {file_name}{file_extension}:{current_line}:{current_column}",
 "vscord.status.state.text.noWorkspaceFound": "üìÇ No workspace.",
 "vscord.status.state.text.viewing": "üìÑ Viewing {file_name}{file_extension}",

 // Error Lens
 "errorLens.codeLensTemplate": "$severity $message",
 "errorLens.enabledDiagnosticLevels": ["error", "warning"],
 "errorLens.fontWeight": "bold",
 "errorLens.gutterIconsEnabled": true,

 // Icon Theme
 "material-icon-theme.activeIconPack": "react",
 "material-icon-theme.enableLogging": false,
 "material-icon-theme.files.color": "#90a4ae",
 "material-icon-theme.folders.color": "#90a4ae",
 "material-icon-theme.folders.theme": "specific",
 "material-icon-theme.hidesExplorerArrows": false,
 "material-icon-theme.opacity": 0.95,
 "material-icon-theme.saturation": 0.9,

 // WSL
 "remote.WSL.debug": false,
 "remote.WSL.fileWatcher.polling": true,
 "remote.WSL.fileWatcher.pollingInterval": 5000,

 // Doki Theme
 "doki.background.anchor": "center",
 "doki.background.enabled": true,
 "doki.background.path": "C:\\.r4lrgx\\vscode\\wallpapers\\bg.png",
 "doki.wallpaper.enabled": true,
 "doki.wallpaper.path": "C:\\.r4lrgx\\vscode\\wallpapers\\wallpaper.png",
 "doki.sticker.css": "z-index:100;background-position:100% 97%",
 // "doki.sticker.path": "C:\\.r4lrgx\\vscode\\stickers\\sticker.png",
 "doki.statusbar.name": "‚ö° Code in less time ‚òï",

 // GitHub Actions
 "github-actions.remote-name": "origin",

 // Image Preview
 "gutterpreview.enableReferenceLookup": true,

 // IntelliCode
 "vsintellicode.python.completionsEnabled": true,
 "intellicodeApiExamples.python.enabled": true,
 "intellicodeCompletions.parseErrorFilter": true,

 // NPM Intellisense
 "npm-intellisense.importES6": true,

 // Path Intellisense
 "path-intellisense.autoSlashAfterDirectory": true,
 "path-intellisense.showHiddenFiles": true,

 // Prettier
 "prettier.arrowParens": "always",
 "prettier.bracketSameLine": false,
 "prettier.bracketSpacing": true,
 "prettier.embeddedLanguageFormatting": "auto",
 "prettier.htmlWhitespaceSensitivity": "css",
 "prettier.jsxSingleQuote": false,
 "prettier.printWidth": 500000,
 "prettier.proseWrap": "preserve",
 "prettier.quoteProps": "as-needed",
 "prettier.semi": true,
 "prettier.singleAttributePerLine": true,
 "prettier.singleQuote": true,
 "prettier.tabWidth": 1,
 "prettier.trailingComma": "es5",
 "prettier.useTabs": false,
 "prettier.vueIndentScriptAndStyle": false
}
